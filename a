// Database crete

CREATE SCHEMA `elephantyachtclub`;
	CREATE TABLE `elephantyachtclub`.`score` (
    `id` INT NOT NULL AUTO_INCREMENT,
    `userName` VARCHAR(45) NOT NULL,
    `scoreInt` VARCHAR(45) NOT NULL,
    `scoreText` VARCHAR(45) NOT NULL,
    PRIMARY KEY (`id`));
    CREATE TABLE `elephantyachtclub`.`subscribe` (
    `id` INT NOT NULL AUTO_INCREMENT,
    `email` VARCHAR(45) NOT NULL,
    PRIMARY KEY (`id`));

